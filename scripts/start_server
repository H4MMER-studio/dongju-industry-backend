# !/bin/bash

cd /home/ubuntu/build/dongju-industry-backend/
aws ssm get-parameters --name "LEVEL" "DB_NAME" "PRODUCT_DB_URL" "PRODUCT_AWS_S3_BUCKET_NAME" "PRODUCT_AWS_ACCESS_KEY_ID" "PRODUCT_AWS_SECRET_ACCESS_KEY" "PRODUCT_ADMIN_EMAIL" "PRODUCT_ADMIN_EMAIL_PASSWORD" "PRODUCT_EMAIL_HOST" "PRODUCT_EMAIL_PORT" > env.json
jq -jr '.Parameters[]|.Name, "=", .Value, "\n"' < env.json >> .env

# poetry run gunicorn --access-logfile /home/ubuntu/logs/access -k uvicorn.workers.UvicornWorker src.main:app --bind 0.0.0.0:80 --workers 2 --daemon
