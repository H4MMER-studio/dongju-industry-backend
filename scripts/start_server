# !/bin/bash

cd /home/ubuntu/build/dongju-industry-backend
sudo aws ssm get-parameters --name "LEVEL" "DB_NAME" "PRODUCT_DB_URL" "PRODUCT_AWS_S3_BUCKET_NAME" "PRODUCT_AWS_ACCESS_KEY_ID" "PRODUCT_AWS_SECRET_AWS_KEY" > env.json
sudo jq -jr '.Parameters[]|.Name, "=", .Value, "\n"' < env.json >> .env
poetry install
poetry run gunicorn -k uvicorn.workers.UvicornWorker --access-logfile ./gunicorn-access.log src.main:app --bind 0.0.0.0:80 --workers 2 --daemon
